<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Source_ID_Must_be_entered</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISBLANK(Source_ID__c ),ISNEW())</errorConditionFormula>
    <errorDisplayField>Source_ID__c</errorDisplayField>
    <errorMessage>Source ID must be entered.</errorMessage>
</ValidationRule>
